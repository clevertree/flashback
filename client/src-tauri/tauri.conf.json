{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png"
    ]
  },
  "productName": "Flashback",
  "mainBinaryName": "Flashback",
  "version": "0.1.0",
  "identifier": "com.clientserver.desktop",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Flashback",
        "width": 800,
        "height": 600,
        "useHttpsScheme": true,
        "permissions": [
          "shell:allow-open",
          "shell:allow-spawn",
          "dialog:allow-save",
          "fs:allow-write-file"
        ]
      }
    ],
    "security": {
      "csp": null
    }
  }
}